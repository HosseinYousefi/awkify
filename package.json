{
  "name": "awkify",
  "version": "1.0.0",
  "description": "Use awk to write scripts to quickly convert group messages to outputs.",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon -x ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HosseinYousefi/awkify.git"
  },
  "keywords": [
    "awk",
    "awkify",
    "telegram",
    "bot"
  ],
  "nodemonConfig": {
    "ignore": [
      "sessions.json"
    ],
    "delay": "2"
  },
  "author": "Hossein Yousefi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HosseinYousefi/awkify/issues"
  },
  "homepage": "https://github.com/HosseinYousefi/awkify#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "telegraf": "^4.3.0",
    "telegraf-session-local": "^2.1.0",
    "temp-write": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
